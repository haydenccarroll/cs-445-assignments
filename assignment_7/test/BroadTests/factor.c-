int sqrt(int x)
{
    int guess; 
    int newguess;

    if x==1 then return 1;
    guess = x/2;
    newguess = (x/guess + guess)/2;
    while guess-newguess>1 do {
        guess = newguess;
        newguess = (x/guess + guess)/2;
    }
    if newguess*newguess>x then newguess=newguess-1;

    return newguess;
}

